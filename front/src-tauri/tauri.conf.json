{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "CarShareX",
  "version": "1.0.0",
  "identifier": "com.carsharex.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "CarShareX",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  }
}
